<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="700.0" prefWidth="950.0" xmlns="http://javafx.com/javafx/23.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.example.controllers.LoginController">
   <children>
      <TextField fx:id="emailField" layoutX="574.0" layoutY="365.0" onKeyReleased="#onEmailTyped" prefHeight="34.0" prefWidth="287.0" promptText="Email" style="-fx-border-color: #FF7F50;">
         <font>
            <Font size="13.0" />
         </font>
      </TextField>
      <PasswordField fx:id="passwordField" layoutX="575.0" layoutY="416.0" onKeyReleased="#onPasswordTyped" prefHeight="34.0" prefWidth="287.0" promptText="password" style="-fx-border-color: #FF7F50;" />
      <Button fx:id="loginButton" layoutX="616.0" layoutY="478.0" mnemonicParsing="false" onAction="#onLoginButtonClicked" prefHeight="34.0" prefWidth="210.0" stylesheets="@../styles/MyStyleSheet.css" text="Login" />
      <ImageView fitHeight="181.0" fitWidth="210.0" layoutX="611.0" layoutY="147.0">
         <image>
            <Image url="@../images/restuarant%20logo.png" />
         </image>
      </ImageView>
      <ImageView fitHeight="150.0" fitWidth="200.0" layoutX="114.0" layoutY="154.0" pickOnBounds="true" preserveRatio="true" />
      <ImageView fitHeight="700.0" fitWidth="561.0" layoutX="1.0">
         <image>
            <Image url="@../images/tasty-serbian-food-arrangement-flat-lay.png" />
         </image>
      </ImageView>
   </children>
</AnchorPane>
